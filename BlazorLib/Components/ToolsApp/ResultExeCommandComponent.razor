<MudDialog>
    <DialogContent>
        <code>@CurrentCommand.FileName @CurrentCommand.Arguments</code>
        <pre>
            @ShellOutput
        </pre>
    </DialogContent>
</MudDialog>

@code {
    [Parameter, EditorRequired]
    public string? ShellOutput { get; set; }

    /// <inheritdoc/>
    [Parameter, EditorRequired]
    public required ExeCommandModelDB CurrentCommand { get; set; }

    [CascadingParameter]
    private IMudDialogInstance MudDialog { get; set; } = default!;
}