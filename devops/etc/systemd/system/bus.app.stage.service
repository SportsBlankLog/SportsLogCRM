[Unit]
Description=bus.app.stage.service

[Service]
WorkingDirectory=/srv/services.stage/RemoteCallLib
ExecStart=/usr/bin/dotnet /srv/services.stage/RemoteCallLib/RemoteCallLib.dll
Restart=always
RestartSec=10
KillSignal=SIGINT
SyslogIdentifier=dotnet-bus-app.stage
User=www-data
Environment=ASPNETCORE_ENVIRONMENT=Staging
Environment=DOTNET_PRINT_TELEMETRY_MESSAGE=false
Environment=TRANSMISSION_QUEUE_NAME_PREFIX=_stage

[Install]
WantedBy=multi-user.target